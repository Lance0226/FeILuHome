GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(SDKROOT)/usr/include/libxml2 $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/GDataXML-HTML" "${PODS_ROOT}/Headers/Public/SVSegmentedControl"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/GDataXML-HTML" -isystem "${PODS_ROOT}/Headers/Public/SVSegmentedControl"
OTHER_LDFLAGS = $(inherited) -ObjC -l"GDataXML-HTML" -l"SVSegmentedControl" -l"xml2" -framework "QuartzCore"
PODS_ROOT = ${SRCROOT}/Pods